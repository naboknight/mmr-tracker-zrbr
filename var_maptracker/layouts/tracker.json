  "tracker_default": {  
    "type": "container",
    "background": "#212121",
    "content": {
      "type": "dock",
      "dropshadow": true,
      "content": [
        {
          "type": "dock",
          "dock": "bottom",
          "v_alignment": "stretch",
          "margin": "0,0,3,0",
		  		"orientation": "vertical",

          "content": [

            {
              "type": "group",
              "header": "Items",
			  "header_content": {
                "type": "button_popup",
                "style": "image",
				"image": "images/unknown.png",
				"height": 25,
                "popup_background": "#FF212121",
                "layout": "options_popup"
              },
              "content": {
                "type": "layout",
                "key": "shared_item_grid"
              }
            },
			{
              "type": "group",
              "header": "Pinned Locations",
              "margin": "3,0,0,0",
              "content": {
                "type": "recentpins",
                "style": "wrap",
                "h_alignment": "stretch",
                "v_alignment": "stretch",
                "orientation": "horizontal",
                "compact": true
              }
            },
            
		    
          ]
        },
       {
              "type": "map",
              "dock": "top",

			  "h_alignment": "stretch",
              "v_alignment": "stretch",
              "map": [
				"termina"
              ]

            }, 
			{

              "type": "group",
              "header": "Pinned Locations",
			  "dock": "bottom",
              "margin": "3,0,0,0",
			  "min_height": 200,
			  "max_height": 500,
              "content": {
                "type": "recentpins",
                "style": "wrap",
                "h_alignment": "stretch",
                "v_alignment": "stretch",
                "orientation": "horizontal",
                "compact": true
              }
              
            },

      ]
    }
  }

}

 


